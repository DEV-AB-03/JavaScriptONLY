var diameterOfBinaryTree = function(root) {
    let result = 0;
    helper(root);
    return result
    
    function helper(root){
        if(!root)return 0;
        
        let left = helper(root.left);
        let right = helper(root.right);
        
        result = Math.max(left + right , result);
        
        return 1 + Math.max(left, right)
    }
};
